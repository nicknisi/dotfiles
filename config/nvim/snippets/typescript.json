{
    "clg": {
        "prefix": "log",
        "body": "console.log($0);"
    },
    "clgc": {
        "description": "console log with colors",
        "prefix": "logc",
        "body": "console.log('%c$1', 'font-weight:bold;color:${2:green}');"
    },
    "describe": {
        "prefix": "des",
        "description": "Describe body for tests",
        "body": [
            "describe('$1', () => {",
            "",
            "});"
        ]
    },
    "it": {
        "prefix": "it",
        "description": "it body for tests",
        "body": [
            "it('$1', () => {",
            "",
            "});"
        ]
    },
    "doc": {
        "prefix": "doc",
        "description": "Documentation comment",
        "body": [
            "/**",
            "* $0",
            "*/"
        ]
    },
    "rfc": {
      "prefix": "rfc",
      "description": "React functional component",
      "body": [
          "export default function $1() {",
          "  return (",
          "    $0",
          "  );",
          "}"
      ]
    }
}
